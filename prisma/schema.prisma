generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Define your models here
model User {
  id              Int     @id @default(autoincrement())
  email           String  @unique
  name            String?
  password        String
  accountVerified Boolean @default(false)
}